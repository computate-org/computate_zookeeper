<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
        <dict>
                <key>Label</key>
                <string>org.zookeeper</string>

                <key>ProgramArguments</key>
                <array>
                        <string>{{ ZOOKEEPER_OPT }}/bin/zookeeper</string>
                        <string>-D</string>
                        <string>{{ ZOOKEEPER_DATA }}</string>
                </array>

                <key>EnvironmentVariables</key>
                <dict>
                </dict>

                <key>KeepAlive</key>
                <true/>

                <key>LowPriorityIO</key>
                <true/>
        </dict>
</plist>
